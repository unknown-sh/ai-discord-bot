# Discord OAuth2 credentials
DISCORD_CLIENT_ID=
DISCORD_CLIENT_SECRET=

# The URL Discord should redirect to after login (should be your backend's /auth/callback endpoint)
DISCORD_REDIRECT_URI=http://localhost:8000/auth/callback

# JWT secret for signing dashboard tokens (use a secure random value in production!)
JWT_SECRET=REPLACE_WITH_A_SECURE_RANDOM_STRING
